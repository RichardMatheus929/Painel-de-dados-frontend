<!DOCTYPE html->
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Painel de Dados Febrace</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Blinker:wght@100;200;300;400;600;700;800;900&family=Cabin:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="./css/index.css" />
    <link rel="icon" href="data:image/png;base64," type="image/x-icon">
    <link href="https://fonts.googleapis.com/css2?family=Barriecito&family=Lato:wght@100&display=swap" rel="stylesheet">  
  </head>
  <body>
   
    <div class="">
      <%- include('./components/navbarTabela.ejs') %>
    </div>


    
    

    <div class="">
      <%- include('./components/mapinha.ejs') %>
    </div>
    
<div class="ano_tabela">
      <h3>Você está acessando o ano de: <%= Ano %></h3>
    </div>
    
      <div class="tabela">
        <table id="input_table">
            <tr>
              <th>Prêmio</th>
              <th>Nome do projeto</th>
              <th>Escola</th>
              <th>Orientador</th>
              <th>Componentes</th> 
              <th>Cidade</th>
            </tr>
            <% for  (let i = 0; i < Usuario.length; i++) { %>
              <tr class= <%= i % 2 === 0 ? 'even' : 'odd' %> >
                <td><%= Usuario[i].Categoria_de_premiação %></td>
                <td><%= Usuario[i].Nome_do_projeto %></td>
                <td><%= Usuario[i].Escola %></td>
                <td><%= Usuario[i].Orientador %></td>
                <td><%= Usuario[i].Componentes %></td>
                <td><%= Usuario[i].Cidade %></td>
              </tr>
            <% } %>
            
        </table>
        

      </div>

   
      <div class="rodape_index">
        <%- include('./components/rodapeAcessar.ejs') %>
      </div>

    <script>

      function redirecionarParaRota(rota) {
        window.location.href = rota;


      }

    const mapa = document.getElementById('mapa_doBR');
    const estados = mapa.querySelectorAll('path');  

      estados.forEach(estado => {
      estado.addEventListener('mouseenter', () => {
      estado.style.fill = 'gray'; // cor do hover
      });

      estado.addEventListener('mouseleave', () => {
      estado.style.fill = ''; // volta da cor
      });
      });

    
    </script>
  </body>
</html>
