    // Função para mover o elemento h1 por 150px no margin-left
function moveHeading() {
  const heading = document.getElementById('h3_move');
  const heading1 = document.getElementById('h5_move');
  
  heading.style.marginLeft = '15px';
  heading1.style.marginLeft = '10px';
}

function moveHeading1(){
  const heading2 = document.getElementById('h1_id');
  heading2.style.marginLeft= '190px'
}
function checkRouteAndMove() {
  const currentRoute = window.location.pathname;

  if (currentRoute === '/inicio') {
    moveHeading();
  }
  if (currentRoute === '/2023') {
    moveHeading1();
  }
  if (currentRoute === '/2018') {
    moveHeading1();
  }
  if (currentRoute === '/2019') {
    moveHeading1();
  }
}

// Verifique a rota ao carregar a página
window.onload = checkRouteAndMove;






