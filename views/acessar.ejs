    function carregarPagina() {
      var path = window.location.pathname;
      
      var rotasDesejadas = [
        "/2023",
        "/2022",
        "/2021",
        "/2020",
        "/2019",
        "/2018"
      ];

      if (rotasDesejadas.includes(path)) {
        var div = document.getElementById("myDiv");
        var footer = document.getElementById("foot")
        
        const txtP = document.getElementById("escolaP_h3");
        const txtS = document.getElementById("escolaS_h3");
        const txtT = document.getElementById("escolaT_h3");

        const imgP = document.getElementById("primeiro");
        const imgS = document.getElementById("segundo");
        const imgT = document.getElementById("terceiro");

        const h1 = document.getElementById("h1_recado");

        footer.style.marginTop = "-20px"

        txtP.style.visibility = "hidden";
        txtS.style.visibility = "hidden";
        txtT.style.visibility = "hidden";

        imgP.style.marginLeft = "346px"
        imgP.style.marginTop = "30px"

        imgS.style.marginLeft = "260px"
        imgS.style.marginTop = "-70px"

        imgT.style.marginLeft = "425px"
        imgT.style.marginTop = "-85.5px"

        h1.innerHTML = "Passe o mouse para visualizar o ranking das escolas do ano de " + path.substring(1) + "."
        h1.style.fontFamily = "Cabin"
        h1.style.fontSize = "25px"
        h1.style.marginTop = "180px"
        h1.style.marginLeft = "-380px"
      }
    }

    document.addEventListener("DOMContentLoaded", carregarPagina);


      function expandDiv(element) {

const primeiro = document.getElementById("primeiro")
const segundo = document.getElementById("segundo")
const terceiro = document.getElementById("terceiro")

const txtP = document.getElementById("escolaP_h3");
const txtS = document.getElementById("escolaS_h3");
const txtT = document.getElementById("escolaT_h3");

const imgP = document.getElementById("primeiro");
const imgS = document.getElementById("segundo");
const imgT = document.getElementById("terceiro");

const h1 = document.getElementById("h1_recado");

var footer = document.getElementById("foot")

if (shouldExpandDiv()) {
imgP.style.marginLeft = "-30px"
imgS.style.marginLeft = "-30px"
imgT.style.marginLeft = "-30px"

footer.style.marginTop = "-70px";

element.style.height = "600px"; 
element.style.marginTop = "-585px"
element.style.zIndex = "2";
element.style.backgroundColor = "rgb(255, 248, 248)";
element.style.cursor = "pointer";

primeiro.style.marginTop = "60px"
segundo.style.marginTop = "170px"
terceiro.style.marginTop = "160px"

txtP.style.marginTop = "50px"
txtS.style.marginTop = "40px"
txtT.style.marginTop = ""

txtP.style.visibility = "visible";
txtS.style.visibility = "visible";
txtT.style.visibility = "visible";

h1.style.visibility = "hidden";
}
}

/// quando tira o mouse de cima da div 
function shrinkDiv(element) {
  const h1 = document.getElementById("h1_recado");

const primeiro = document.getElementById("primeiro")
const segundo = document.getElementById("segundo")
const terceiro = document.getElementById("terceiro")

const txtP = document.getElementById("escolaP_h3");
const txtS = document.getElementById("escolaS_h3");
const txtT = document.getElementById("escolaT_h3");

const imgP = document.getElementById("primeiro");
const imgS = document.getElementById("segundo");
const imgT = document.getElementById("terceiro");

var footer = document.getElementById("foot")

if (shouldExpandDiv()) {

element.style.height = "280px"; 
element.style.marginTop = "-315px"

primeiro.style.marginTop = "20px"
segundo.style.marginTop = "30px"
terceiro.style.marginTop = "40px"

footer.style.marginTop = "-20px"

        txtP.style.visibility = "hidden";
        txtS.style.visibility = "hidden";
        txtT.style.visibility = "hidden";

        h1.style.visibility = "visible";
        
        imgP.style.marginLeft = "346px"
        imgP.style.marginTop = "30px"

        imgS.style.marginLeft = "260px"
        imgS.style.marginTop = "-70px"

        imgT.style.marginLeft = "425px"
        imgT.style.marginTop = "-85.5px"

        h1.innerHTML = "Passe o mouse para visualizar o ranking das escolas do ano de " + path.substring(1) + "."
        h1.style.fontFamily = "Cabin"
        h1.style.fontSize = "25px"
        h1.style.marginTop = "180px"
        h1.style.marginLeft = "-410px"

}
}
function shouldExpandDiv() {
const allowedRoutes = ["/2023", "/2022", "/2021", "/2020", "/2019", "/2018"];
const currentPath = window.location.pathname;
return allowedRoutes.includes(currentPath);
}
    




